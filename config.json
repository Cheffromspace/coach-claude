{
  "logging": {
    "console_level": "DEBUG",
    "file_level": "DEBUG",
    "debug_level": "DEBUG",
    "mcp_loggers": {
      "mcp": "DEBUG",
      "mcp_client.server.server_manager": "DEBUG",
      "mcp_client.server.stdio": "DEBUG",
      "mcp_client.server.memory": "DEBUG"
    }
  },
  "mcpServers": {
    "fetch": {
      "command": "/home/jonflatt/.local/bin/uvx",
      "args": ["mcp-server-fetch"],
      "env": {
        "UV_HOME": "/home/jonflatt/.uv",
        "UV_SYSTEM_PYTHON": "/home/jonflatt/.python-version"
      }
    },
    "mcp-installer": {
      "command": "npx",
      "args": ["-y", "@anaisbetts/mcp-installer"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/jonflatt/mcp-client-files",
        "/home/jonflatt/source/mcp-client"
      ]
    }
  }
}
